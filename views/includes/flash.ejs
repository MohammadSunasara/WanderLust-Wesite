<% if (message && message.length) {%>

<div class="alert alert-success alert-dismissible fade show mt-4 col-6 offset-3" role="alert"><%=message%>
<button class="btn-close" type="button" aria-label="close"data-bs-dismiss="alert"></button>
</div>

<%}%>


<% if (error && error.length) {%>

<div class="alert alert-danger alert-dismissible fade show mt-4 col-6 offset-3" role="alert"><%=error%>
<button class="btn-close" type="button" aria-label="close"data-bs-dismiss="alert"></button>
</div>

<%}%>